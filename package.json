    "@vue/compiler-dom": "^3.5.8",
    "@vue/compiler-sfc": "^3.5.7",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.28.0",
